<?php

	# VISUALIZADOR HTML	


	
		
	# CONTENIDO (INPUT)
	$html_contenido = '';
	switch($permissions) {
			
		case 0	:	$html_contenido .= "";		
					break;
					
		case ($permissions>=1) :
			foreach ($dato as $uri) {
				$html_contenido .= '<a class="uri_link" href="'.$uri->iri.'" target="_blank" title="'.$uri->iri.'"> </a>';
			}

			break;
					
	}	

	
	# PRINT HTML	
	print $html_contenido;